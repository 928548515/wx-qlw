/* pages/my/page/pet/pet.wxss */
page{
  background-color: #eee;
}
.conainer{
  padding: 10rpx 10rpx 0 10rpx;
}
.conainer>view{
  margin-bottom: 10rpx;

}
.dog-item{
  background-color: #fff;
  font-size: 28rpx;
  color: #bbb;
  display: flex;
  align-items: space-between;
  flex-flow: row nowrap;
  height: 196rpx;
  padding: 13rpx;
  box-sizing: border-box;
  line-height: 46rpx;
  margin-bottom: 10rpx;
}

.dog-pic{
  height: 100%;
  width: 160rpx;
  margin-right: 30rpx;
  background-color:#ddd;
}
.info{
  display: flex;
  flex-flow: column nowrap;
  justify-content: space-between;
  width:375rpx;
}
.info>view>text:nth-child(2){
   margin-left: 30rpx;
  margin-right: 30rpx;
}
.basic-infos{
  display: flex;
  align-items: space-between;
}
.basic-infos text:first-child{
  font-size: 32rpx;
  color: #000;
}
.date-infos{
  color: #fff;
  font-size: 25rpx;
}
.date-infos>text{
  padding: 5rpx 10rpx;
}
.date-infos>text:first-child{
  background-color: #FD986B;
}
.date-infos>text:last-child{
  background-color: #FD6459;
}

.status{
  display: flex;
  flex-flow: column nowrap;
  justify-content: center  ;
  align-items: flex-end;
  margin-left: auto;
}
.status>text{
  margin-right: 10rpx;
  margin-bottom: 20rpx;
}
.stateClid{
  background-color: #4BC9DA;
  color: #FFF;
  padding: 3rpx 12rpx;
  border-radius: 15rpx;
}
.isExpire{
  color: #139918;
}
.expire{
  color: #FD6459;
}
.bindDevie:active{
  background-color: #4BC9da;
}
.lint-one{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width:221rpx;
}
.age-infos{
  line-height: 30rpx;
}
.bgceee{
  background-color:#6CA6CD;
}