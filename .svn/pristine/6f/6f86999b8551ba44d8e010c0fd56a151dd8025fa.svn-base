/* pages/my/page/appointment/appointment.wxss */

.map {
  height: 500rpx;
  width: 100%;
  padding-bottom: 70rpx;
}

.map_box2 {
  position: absolute;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 0px;
}

.map_box {
  width: 100%;
  height: 100%;
  position: relative;
}

#navi_map {
  width: 100%;
  height: 100%;
  z-index: 1;
}

.list {
  border: solid 1rpx #ccc;
}

/* 列表 */

.item {
  display: flex;
  align-items: center;
  font-size: 36rpx;
  padding: 22rpx;
  position: relative;
  border-bottom: solid 1rpx #ccc;
  background-color: #fff;
}

image {
  height: 188rpx;
  width: 200rpx;
  margin-right: 30rpx;
}

.content {
  display: flex;
  flex-flow: column nowrap;
  font-size: 30rpx;
  height: 188rpx;
  justify-content: space-around;
  width: 500rpx;
}

.titile {
  width: 358rpx;
  line-height: 33rpx;
}

.add {
  font-size: 30rpx;
  color: #aaa;
}

.phone {
  display: flex;
}

.content image {
  height: 40rpx;
  width: 40rpx;
  margin-right: 10rpx;
}

.add {
  display: flex;
  font-size: 27rpx;
  flex-flow: row nowrap;
  line-height: 26rpx;
}

.but {
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  background-color: #49c8d5;
  border-radius: 10rpx;
  width: 100rpx;
  text-align: center;
  line-height: 50rpx;
  color: #fff;
  font-size: 34rpx;
}

.tar {
  height: 70rpx;
  font-size: 14px;
  display: flex;
  text-align: center;
  background-color: #fff;
  justify-content: space-around;
  position: relative;
}
.tar_items-last{
  position: absolute;
}

.tar_items {
  display: flex;
  flex: 5;
  line-height: 70rpx;
  justify-content: center;
}

.tar_items image {
  width: 48rpx;
  height: 48rpx;
  margin: 0;
}

.tar_item_i {
  display: flex;
  align-items: center;
  padding-right: 10rpx;
}

.max {
  position: absolute;
  z-index: 99999;
  right: 0;
  top: 0;
  height: 50rpx;
  width: 50rpx;
}
.tar_items_last{
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 10rpx;
}

.btnImg {
  height: 50rpx;
  width: 50rpx;
  margin: 0;
  background-color: lightcyan;
  z-index: 2;
}
.btnImg:active{
  background-color: lightblue;
}
.btnImg image{
  margin: 0;
  height: 50rpx;
  width: 50rpx;
}
.img-wrap {
    position: relative;
    width: 100%;
    height: 100%;
}
.img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

